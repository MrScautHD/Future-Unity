        <ImplicitUsings>disable</ImplicitUsings>
        <Nullable>disable</Nullable>
        <AssemblyName>Stellution.Client</AssemblyName>
        <RootNamespace>Stellution.Client</RootNamespace>
    </PropertyGroup>
    
    <ItemGroup>
